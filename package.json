{
  "name": "warehouse-terminal-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:clean": "npm install && rm -rf dist && npm run build",
    "start-server": "npm run build:clean && http-server dist -p 8088",
    "start-windows-server": "npm install & npm run build & http-server dist -p 8088"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^3.4.7",
    "@vueuse/core": "^8.4.2",
    "axios": "^0.27.2",
    "http-server": "^14.1.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4",
    "vue3-easy-data-table": "^1.4.17"
  },
  "devDependencies": {
    "@iconify/vue": "^4.0.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "autoprefixer": "^10.4.8",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^3.0.9",
    "vite-plugin-pages": "^0.25.0",
    "vue-tsc": "^0.39.5"
  }
}
